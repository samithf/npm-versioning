{
  "name": "versioning",
  "version": "1.3.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "commit": "npx git-cz && npm run release"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "git add .",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "husky": "^3.0.0",
    "standard-version": "^6.0.1"
  }
}
