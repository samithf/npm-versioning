{
  "name": "versioning",
  "version": "1.1.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "commit": "npx git-cz"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
